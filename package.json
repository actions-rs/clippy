{
    "name": "rust-clippy",
    "version": "0.1.0",
    "private": false,
    "description": "Run clippy and annotate the diff with errors and warnings",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf ./dist/* && ncc build src/main.ts --minify",
        "format": "prettier --write 'src/**/*.ts'",
        "lint": "tsc --noEmit && eslint 'src/**/*.ts'",
        "watch": "rm -rf ./dist/* && ncc build src/main.ts --watch",
        "test": "jest -c jest.config.json --passWithNoTests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/actions-rs/clippy.git"
    },
    "keywords": [
        "actions",
        "rust",
        "cargo",
        "clippy"
    ],
    "author": "actions-rs",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/actions-rs/clippy/issues"
    },
    "dependencies": {
        "@actions-rs/core": "0.0.9",
        "@actions/core": "^1.2.4",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.5",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "@zeit/ncc": "^0.22.1",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.0.1",
        "jest-circus": "^26.0.1",
        "mock-env": "^0.2.0",
        "npm-check-updates": "^5.0.0",
        "prettier": "^2.0.5",
        "ts-jest": "^25.5.1",
        "typescript": "^3.8.3"
    }
}
